{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/agent.html",
      "dest": "/public/agent.html"
    },
    {
      "src": "/results.html",
      "dest": "/public/results.html"
    },
    {
      "src": "/results",
      "dest": "/public/results.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}

